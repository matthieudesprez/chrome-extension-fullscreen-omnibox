{
  "manifest_version": 2,
  "name": "Fullscreen Toolbar URL Access",
  "description": "Access Toolbar URL in Fulllscreen Mode",
  "version": "1.1",
  "browser_action": {
    "default_icon": "images/icon-16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "history",
    "bookmarks"
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      }
    }
  }
}
